# Break and continue examples

print "=== Break Example ==="
for num in range(1, 20) {
    if num > 10 {
        break
    }
    print num
}

print "=== Continue Example ==="
for num in range(1, 10) {
    if num == 5 {
        continue
    }
    print num
}

print "=== Combined Example ==="
for num in range(1, 15) {
    if num > 12 {
        break
    }
    if num == 7 {
        continue
    }
    print num
}
